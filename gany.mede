name: "gany"
description: "A simple package manager."
license: "GPL-3.0"
pubDate: 2020-08-15
dependencies: []
install: |
  echo $PWD
  ls
  git clone https://github.com/MadeByEmil/gany.git
  cd gany-master
  dotnet build --configuration Release --runtime linux-x64
  cd ./bin/Release/netcoreapp3.1
  cp gany /usr/bin
  chmod +x /usr/bin/gany
remove: |
  rm /usr/bin/gany
